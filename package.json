{
  "name": "winston-knex",
  "version": "0.2.0",
  "description": "Winston Transport for Knex JS",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "types": "build/src/index.d.ts",
  "typings": "build/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "build/index.cjs",
    "build/index.js",
    "build/src/index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.ts --ignore-path .gitignore .",
    "coverage": "nyc npm run mocha",
    "mocha": "mocha -r ts-node/register test/**/*.spec.ts --exit",
    "pretest": "rimraf build && npm run lint:fix && npm run build",
    "test": "npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldy505/winston-knex.git"
  },
  "keywords": [
    "knex",
    "winston",
    "winston-3.x",
    "winston-3",
    "transport",
    "winston-transport"
  ],
  "author": "Reinaldy Rafli <hi@reinaldyrafli.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldy505/winston-knex/issues"
  },
  "homepage": "https://github.com/aldy505/winston-knex#readme",
  "engines": {
    "node": ">=10"
  },
  "directories": {
    "lib": "src/",
    "test": "test/"
  },
  "dependencies": {
    "knex": "^0.95.6",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-typescript": "7.13.0",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@types/dotenv": "8.2.0",
    "@types/node": "15.6.2",
    "@types/pg": "8.6.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "abstract-winston-transport": "0.5.1",
    "dotenv": "10.0.0",
    "eslint": "7.27.0",
    "eslint-config-xo-space": "^0.27.0",
    "mysql2": "2.2.5",
    "nyc": "15.1.0",
    "pg": "8.6.0",
    "rimraf": "3.0.2",
    "rollup": "2.50.5",
    "rollup-plugin-typescript2": "0.30.0",
    "source-map-support": "0.5.19",
    "ts-node": "10.0.0",
    "tslib": "2.2.0",
    "typescript": "4.3.2",
    "uvu": "^0.5.1",
    "winston": "3.3.3"
  }
}
